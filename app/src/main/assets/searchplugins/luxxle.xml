<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Luxxle</ShortName>
<InputEncoding>UTF-8</InputEncoding>
<Image width="16" height="16">data:image/png;base64,</Image>
  <Url type="text/html" method="get" template="https://www.luxxle.com/luxsearch" >
  <Param name="q" value="{searchTerms}"/>
  </Url>
  <Url type="application/x-suggestions+json" template="https://www.luxxle.com/autocomplete.php" >
   <Param name="q" value="{searchTerms}"/>
   <Param name="locale" value="{moz:locale}"/>
  </Url>
 <SearchForm>https://www.luxxle.com</SearchForm>
</SearchPlugin> 
